{
  "name": "sphinx_shiguredo_theme",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "serve ./example/build/html",
    "build": "yarn _build/sass && yarn _build/copyfile",
    "_build/sass": "sass ./src/sass/theme.scss ./sphinx_shiguredo_theme/static/css/theme.css",
    "_build/copyfile": "node -e \"require('fs').copyFileSync('./node_modules/bootstrap/dist/js/bootstrap.min.js', './sphinx_shiguredo_theme/static/js/bootstrap.min.js')\"",
    "fmt": "prettier --write ./src && prettier --write ./sphinx_shiguredo_theme/*.html"
  },
  "devDependencies": {
    "bootstrap": "^5.0.0-beta1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.5",
    "serve": "^11.3.2"
  }
}
